		<NodeBuilder class="MonoDevelop.PackageManagement.NodeBuilders.NuGetConfigFilesSolutionNodeBuilder" />
		<NodeBuilder class="MonoDevelop.PackageManagement.NodeBuilders.NuGetConfigFileNodeBuilder" />
